{
  "expo": {
    "name": "sunrinthon-app",
    "slug": "sunrinthon-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "scheme": "sunrinthon-app",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sunrinthonapp",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.sunrinthonapp",
            "CFBundleURLSchemes": ["sunrinthon-app", "com.sunrinthonapp"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sunrinthonapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": {
            "scheme": "sunrinthon-app"
          }
        },
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": {
            "scheme": "com.sunrinthonapp"
          }
        }
      ]
    },
    "plugins": [
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2ebdd2dc-03bd-4c3a-8344-24d6dac91bb5"
      }
    }
  }
}
