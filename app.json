{
  "expo": {
    "name": "sunrinthon-app",
    "slug": "sunrinthon-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "sunrinthon-app",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sunrinthonapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sunrinthonapp"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "2ebdd2dc-03bd-4c3a-8344-24d6dac91bb5"
      }
    }
  }
}
